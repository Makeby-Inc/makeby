{
	"extends": "@configs/typescript/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"plugins": [{ "name": "next" }],
		"paths": {
			"#/*": ["./src/domains/*"],
			"~/*": ["./src/*"]
		}
	},
	"include": [
		"svg.d.ts",
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules"]
}
